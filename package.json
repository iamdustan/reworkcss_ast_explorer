  "license": "MIT",
  "devDependencies": {
    "brfs": "^1.1.1",
    "browserify": "^9.0.3",
    "envify": "^3.2.0",
    "http-server": "^0.8.0",
    "reactify": "^1.1.0",
    "uglify-js": "^2.4.16",
    "watchify": "^2.3.0"
  },
  "dependencies": {
    "codemirror": "^5.1.0",
    "css": "^2.2.0",
    "keypress": "git://github.com/dmauro/Keypress",
    "pubsub-js": "^1.4.2",
    "react": "^0.13.1",
    "react-tools": "^0.13.x"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ],
      "envify",
      "brfs"
    ]
  },
  "scripts": {
    "start": "http-server",
    "build": "NODE_ENV=production browserify src/app.js | uglifyjs -mc -b beautify=false,ascii-only=true > app.js",
    "watch": "NODE_ENV=development watchify src/app.js -o app.js"
  }
}
