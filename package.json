{
  "name": "reworkcss_ast_demo",
  "version": "0.1.0",
  "description": "Online AST explorer of Rework/cssâ€™s AST structure",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamdustan/reworkcss_ast_explorer.git"
  },
  "authors": [
    "Dustan Kasten",
    "Felix Kling"
